{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-console": "off",
        "no-case-declarations": "off",
        "no-restricted-syntax": "off",
        "guard-for-in": "off",
        "no-continue": "off",
        "no-plusplus": "off"
    }
}